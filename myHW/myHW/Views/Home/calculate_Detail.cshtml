@using myHW.Helpers
@model IEnumerable<myHW.Models.calculateViewModels>

@{
    Layout = null;
}

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>類別</th>
                <th>日期</th>
                <th>金額</th>
            </tr>
          
           @foreach (var items in @Model)
           {
            <tr>
                <td>@items.ID</td>
                <td class="@HtmlExtensions.setColorCategoryyy(items.Type)">@items.Type</td>
                <td>@string.Format("{0:yyyy-MM-dd}", items.CreateTime)</td>
                <td>@items.Money</td>
            </tr>
           }

        </table>
    </div>
</div>
